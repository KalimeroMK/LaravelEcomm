{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.4",
    "bacon/bacon-qr-code": "*",
    "barryvdh/laravel-dompdf": "*",
    "darkaonline/l5-swagger": "*",
    "guzzlehttp/guzzle": "*",
    "intervention/image": "*",
    "jeremykenedy/laravel-blocker": "dev-master",
    "jeremykenedy/laravel-logger": "*",
    "kalimeromk/filterable": "*",
    "kalimeromk/laravel-translation-manager": "*",
    "kalnoy/nestedset": "*",
    "lab404/laravel-impersonate": "*",
    "laravel/framework": "^12.0",
    "laravel/helpers": "*",
    "laravel/horizon": "^5.33",
    "laravel/sanctum": "*",
    "laravel/socialite": "*",
    "laravel/telescope": "*",
    "laravel/tinker": "^2.10.1",
    "laravel/ui": "*",
    "league/omnipay": "*",
    "maatwebsite/excel": "*",
    "nwidart/laravel-modules": "*",
    "omnipay/paypal": "*",
    "pragmarx/google2fa": "*",
    "pragmarx/google2fa-laravel": "*",
    "pragmarx/google2fa-qrcode": "*",
    "predis/predis": "*",
    "psr/simple-cache": "*",
    "pusher/pusher-php-server": "*",
    "spatie/laravel-cookie-consent": "*",
    "spatie/laravel-feed": "*",
    "spatie/laravel-honeypot": "*",
    "spatie/laravel-medialibrary": "*",
    "spatie/laravel-permission": "*",
    "spatie/laravel-robots-middleware": "^1.4",
    "spatie/laravel-sitemap": "^7.3",
    "spatie/schema-org": "^3.23",
    "stripe/stripe-php": "^16.6"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^3.5",
    "fakerphp/faker": "^1.23",
    "larastan/larastan": "^3.0",
    "laravel/pail": "^1.2.2",
    "laravel/pint": "^1.13",
    "laravel/sail": "^1.41",
    "mockery/mockery": "^1.6",
    "nunomaduro/collision": "^8.0",
    "phpunit/phpunit": "^12.0",
    "rector/rector": "^2.0.10",
    "reliese/laravel": "^1.4"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/KalimeroMK/laravel-blocker"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/",
      "Modules\\": "Modules/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "Modules\\": "Modules/"
    }
  },
  "files": [
    "Modules/Core/Helpers/Helper.php"
  ],
  "scripts": {
    "phpstan": "php -d memory_limit=-1 vendor/bin/phpstan analyse",
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true,
      "wikimedia/composer-merge-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
