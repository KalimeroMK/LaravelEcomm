{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "bacon/bacon-qr-code": "*",
        "barryvdh/laravel-dompdf": "*",
        "darkaonline/l5-swagger": "^8.5",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.7",
        "jeremykenedy/laravel-blocker": "dev-master",
        "jeremykenedy/laravel-logger": "9.*",
        "jeroen-g/explorer": "^3.13",
        "kalimeromk/filterable": "^1.5",
        "kalimeromk/laravel-translation-manager": "^1.5",
        "kalnoy/nestedset": "^v6.0.4",
        "lab404/laravel-impersonate": "*",
        "laravel/framework": "^11.0",
        "laravel/helpers": "*",
        "laravel/sanctum": "^4.0",
        "laravel/scout": "^10.8",
        "laravel/socialite": "^5.12",
        "laravel/telescope": "*",
        "laravel/tinker": "^2.8",
        "laravel/ui": "*",
        "league/omnipay": "*",
        "maatwebsite/excel": "^3.1",
        "nwidart/laravel-modules": "^10.0.5",
        "omnipay/paypal": "*",
        "pragmarx/google2fa": "*",
        "pragmarx/google2fa-laravel": "^2.2",
        "pragmarx/google2fa-qrcode": "^3.0",
        "predis/predis": "^2.2",
        "psr/simple-cache": "*",
        "pusher/pusher-php-server": "*",
        "spatie/laravel-cookie-consent": "*",
        "spatie/laravel-feed": "*",
        "spatie/laravel-honeypot": "*",
        "spatie/laravel-medialibrary": "^11.1",
        "spatie/laravel-permission": "*",
        "spatie/laravel-robots-middleware": "*",
        "spatie/laravel-sitemap": "*",
        "spatie/schema-org": "*",
        "stripe/stripe-php": "*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "*",
        "barryvdh/laravel-ide-helper": "^3.0",
        "fakerphp/faker": "^1.9.1",
        "larastan/larastan": "^2.9",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^10.1",
        "reliese/laravel": "^1.3",
        "spatie/laravel-ignition": "^2.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/KalimeroMK/laravel-blocker"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Modules\\": "Modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "files": [
        "Modules/Core/Helpers/Helper.php"
    ],
    "scripts": {
        "phpstan": "php -d memory_limit=-1 vendor/bin/phpstan analyse",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
